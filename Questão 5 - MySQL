create database CLIENTES;

use clientes;

create table CLIENTES (
    CPF VARCHAR(11) PRIMARY KEY,
    NOME VARCHAR(100),
    IDADE INT );

create table TELEFONES (
    CPF_CLIENTE VARCHAR(11) PRIMARY KEY,
    DDD VARCHAR(3),
    TELEFONE VARCHAR(10),
    FOREIGN KEY (CPF_CLIENTE) REFERENCES CLIENTES(CPF) );
